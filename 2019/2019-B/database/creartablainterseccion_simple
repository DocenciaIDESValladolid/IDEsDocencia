--Solo con las variables que nos dan datos interesantes para mostrar en la salida
CREATE TABLE interseccion(
   ID  SERIAL PRIMARY KEY,
   name char(50),
   --Aeropuertos 
   tip_area int,
   tip_areaD char(60),
   nombre char(200),
   --Aves
   SITE_NAME char(100),
   AC char(150),
   --Parques
   SITENAME char(254),
   FIGURA_LP char(100),
   CCAA_N_ENP char(254),
   --
   geom geometry(MULTILINESTRING,3857)
   
);
